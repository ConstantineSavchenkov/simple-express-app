{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "new-cap": 0,
        "space-before-blocks": [
            "warn",
            "always"
        ],
        "object-curly-spacing": [
            "warn",
            "always"
        ]
    }
}